    "eslint-plugin-standard": "^3.1.0",
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "7.0.0",
    "babel-loader": "^8.0.2",
